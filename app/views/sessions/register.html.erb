<%= stylesheet_link_tag "custom", :media => "all" %>
<h1>新規登録</h1>
<%= form_for(:session, url: register_path) do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>  
  </div>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>  
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>  
  </div>
  <%= f.submit "登録",:class => "button_default" %>
<% end %>