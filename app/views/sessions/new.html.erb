<%= stylesheet_link_tag "custom", :media => "all" %>
<h1>ログイン</h1>
<%= form_for(:session, url: login_path) do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>  
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>  
  </div>
  <%= f.submit "Log in",:class => "button_default" %>
<% end %>